                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginStart="@dimen/_5sdp"
                                    android:inputType="numberDecimal"
                                    android:id="@+id/total_plastic_waste_revenue_generated"
                                    android:fontFamily="@string/tamil_font"
                                    android:background="@color/full_transparent"
                                    android:singleLine="true"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_12sdp"
                                    android:visibility="visible"
                                    style="@string/font_normal"
                                    android:importantForAutofill="no" />

                            </RelativeLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_marginTop="@dimen/_10sdp">

                            <TextView
                                android:id="@+id/waste_dump_photos_list"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_10sdp"
                                android:text="Waste Dump Photos List"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/_12sdp"
                                android:fontFamily="@string/tamil_font"/>

                            <androidx.recyclerview.widget.RecyclerView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:id="@+id/waste_dump_recycler"
                                android:layout_marginTop="@dimen/_10sdp"/>

                        </LinearLayout>

                        <RelativeLayout
                            android:layout_marginTop="@dimen/_10sdp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="bottom"
                            android:layout_marginLeft="@dimen/_30sdp"
                            android:layout_marginRight="@dimen/_30sdp"
                            android:background="@drawable/new_bg_log_in_btn_purple"
                            android:layout_marginBottom="@dimen/_15sdp">

                            <Button
                                android:id="@+id/save"
                                android:layout_centerHorizontal="true"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/save"
                                android:fontFamily="@string/tamil_font"
                                android:background="@color/full_transparent"
                                android:textAllCaps="false"
                                android:textColor="@color/white">
                            </Button>

                        </RelativeLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:id="@+id/clear_waste_dump_recycler_layout"
                        android:layout_marginTop="@dimen/_10sdp">

                        <TextView
                            android:id="@+id/clear_waste_dump_recycler_text"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:text="Cleared Waste Dump Photos List"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/_12sdp"
                            android:fontFamily="@string/tamil_font"/>

                        <androidx.recyclerview.widget.RecyclerView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/clear_waste_dump_recycler"
                            android:layout_marginTop="@dimen/_10sdp"/>

                    </LinearLayout>

                </LinearLayout>

            </androidx.core.widget.NestedScrollView>

        </RelativeLayout>

    </RelativeLayout>

</layout>