<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="activity"
            type="com.nic.thooimaikaavalar.activity.SWMActivity.AddCarriedOutsScreen" />
    </data>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bg_icon_new_1"
        tools:context=".activity.NewMainPage">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#66B8B4B4">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_50sdp"
                android:id="@+id/toolbar"
                android:background="@color/colorPrimaryDark">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/back_img"
                    android:src="@mipmap/back_arrow"
                    android:tint="@color/white"
                    android:padding="@dimen/_10sdp"
                    android:layout_centerVertical="true"
                    android:contentDescription="@string/back_icon"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/title"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_13sdp"
                    android:text="@string/activity_carried_out"
                    android:fontFamily="@string/tamil_font"
                    android:layout_centerInParent="true"
                    android:textStyle="bold"/>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/waste_dump_img"
                    android:src="@drawable/ic_boi_degradable_waste"
                    android:padding="@dimen/_10sdp"
                    android:tint="@color/white"
                    android:layout_centerVertical="true"
                    android:layout_alignParentEnd="true"
                    android:contentDescription="@string/back_icon"/>

            </RelativeLayout>

            <androidx.core.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/toolbar">

                <LinearLayout
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/toolbar"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/carried_out_details_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        tools:ignore="UselessParent">

                        <TextView
                            android:id="@+id/please_choose_date"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/padding"
                            android:text="@string/choose_date"
                            android:fontFamily="@string/tamil_font"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/_12sdp"
                            />
                        <RelativeLayout
                            android:id="@+id/choose_date_layout"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/header"
                            android:layout_marginTop="@dimen/padding"
                            android:background="@drawable/rectangle_box"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentStart="true"
                                android:layout_centerInParent="true"
                                android:layout_marginStart="15dp"
                                android:textColor="@android:color/white"
                                android:textSize="@dimen/_12sdp"
                                android:visibility="visible"
                                android:fontFamily="@string/tamil_font"
                                tools:ignore="RelativeOverlap" />

                            <Spinner
                                android:id="@+id/choose_date_spinner"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentEnd="true"
                                android:layout_centerInParent="true"
                                android:layout_marginLeft="10dp"
                                android:layout_marginRight="10dp"
                                android:gravity="center" />

                        </RelativeLayout>

                        <TextView
                            android:id="@+id/please_total_quantity_of_waste"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/padding"
                            android:text="@string/total_quantity_of_waste_collected_in_the_panchayat_in_kg"
                            android:fontFamily="@string/tamil_font"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/_12sdp"
                            />
                        <RelativeLayout
                            android:id="@+id/total_quantity_of_waste_layout"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/header"
                            android:layout_marginTop="@dimen/padding"
                            android:background="@drawable/rectangle_box"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/_5sdp"
                                android:id="@+id/garbage_icon"
                                android:src="@drawable/ic_garbage"
                                android:layout_centerVertical="true"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:contentDescription="garbage_icon" />

                            <EditText
                                android:layout_toEndOf="@+id/garbage_icon"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:inputType="numberDecimal"
                                android:id="@+id/total_quantity_of_waste"
                                android:fontFamily="@string/tamil_font"
                                android:background="@color/full_transparent"
                                android:singleLine="true"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_12sdp"
                                android:visibility="visible"
                                style="@string/font_normal"
                                android:importantForAutofill="no" />

                        </RelativeLayout>


                        <TextView
                            android:id="@+id/please_quantity_of_bio_degradable_waste"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/padding"
                            android:text="@string/quantity_of_the_bio_degradable_waste_collected_in_kg"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/_12sdp"
                            android:fontFamily="@string/tamil_font"/>
