                                android:text="@string/plastic_waste_management"
                                android:textColor="@color/darkblue"
                                android:textSize="@dimen/_12sdp"
                                android:fontFamily="@string/tamil_font"/>

                            <TextView
                                android:layout_marginStart="@dimen/_10sdp"
