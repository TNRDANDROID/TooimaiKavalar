                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_12sdp"
                                    android:visibility="visible"
                                    style="@string/font_normal"
                                    android:importantForAutofill="no" />

                            </RelativeLayout>

                            <TextView
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_marginEnd="@dimen/_10sdp"
                                android:id="@+id/please_total_revenue_generated"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_10sdp"
                                android:text="@string/total_revenue_generated_in_lakhs"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/_12sdp"
                                android:fontFamily="@string/tamil_font"/>
                            <RelativeLayout
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_marginEnd="@dimen/_10sdp"
                                android:layout_marginBottom="@dimen/_10sdp"
                                android:id="@+id/total_revenue_generated_layout"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/header"
                                android:layout_marginTop="@dimen/padding"
                                android:background="@drawable/rectangle_box"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:padding="@dimen/_5sdp"
                                    android:id="@+id/total_revenue_generated_icon"
                                    android:src="@drawable/ic_revenue_icon"
                                    android:layout_centerVertical="true"
                                    android:layout_marginStart="@dimen/_5sdp"
                                    android:contentDescription="garbage_icon" />

                                <EditText
