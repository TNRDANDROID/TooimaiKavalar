                                android:layout_width="match_parent"
                                android:layout_height="@dimen/header"
                                android:layout_marginTop="@dimen/padding"
                                android:background="@drawable/rectangle_box"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:padding="@dimen/_5sdp"
                                    android:id="@+id/quantity_of_compostable_waste_recycle_icon"
                                    android:src="@drawable/ic_plastic_waste_icon"
                                    android:layout_centerVertical="true"
                                    android:layout_marginStart="@dimen/_5sdp"
                                    android:contentDescription="garbage_icon" />

                                <EditText
                                    android:layout_toEndOf="@+id/quantity_of_compostable_waste_recycle_icon"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginStart="@dimen/_5sdp"
                                    android:inputType="numberDecimal"
                                    android:id="@+id/quantity_of_compostable_waste_recycle"
