                            android:id="@+id/quantity_of_bio_degradable_waste_layout"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/header"
                            android:layout_marginTop="@dimen/padding"
                            android:background="@drawable/rectangle_box"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="@dimen/_5sdp"
                                android:id="@+id/quantity_of_bio_degradable_waste_icon"
                                android:src="@drawable/ic_boi_degradable_waste"
                                android:layout_centerVertical="true"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:contentDescription="@string/specify_icon" />

                            <EditText
                                android:layout_toEndOf="@+id/quantity_of_bio_degradable_waste_icon"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:inputType="numberDecimal"
                                android:id="@+id/quantity_of_bio_degradable_waste"
                                android:fontFamily="@string/tamil_font"
                                android:background="@color/full_transparent"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_12sdp"
                                android:visibility="visible"
                                style="@string/font_normal"
                                android:importantForAutofill="no" />

                        </RelativeLayout>

                        <TextView
                            android:id="@+id/please_total_quantity_of_compost_generated"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:text="@string/total_quantity_of_compost_generated_from_the_community_compost_pit_in_kg"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/_12sdp"
                            android:fontFamily="@string/tamil_font"/>
                        <RelativeLayout
                            android:id="@+id/total_quantity_of_compost_generated_layout"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/header"
                            android:layout_marginTop="@dimen/padding"
                            android:background="@drawable/rectangle_box"
                            android:orientation="vertical">

                            <ImageView
